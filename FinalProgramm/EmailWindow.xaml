<Window x:Class="FinalProgramm.EmailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProgramm"
        mc:Ignorable="d"
        Title="Email" Height="150" Width="200">
    <StackPanel>
        <TextBlock Text="Enter your Email" TextAlignment="Center" Margin="10 10 10 -10"/>
        <TextBox x:Name="EmailTextBox" Margin="10 20 10 20"/>
        <Button Click="SubmitButton_Click" Content="OK" Margin="10 0 10 0"/>
    </StackPanel>
</Window>
